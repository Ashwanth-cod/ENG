{
  "name": "eng-programming-language",
  "displayName": "Eng programming language",
  "description": "The easiest natural language based programming language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Eng Light Theme",
        "uiTheme": "vs",
        "path": "./themes/Eng-Light-color-theme.json"
      },
      {
        "label": "Eng Dark Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/Eng-Dark-color-theme.json"
      }
    ],
    "languages": [{
      "id": "eng",
      "aliases": ["Eng", "eng"],
      "extensions": [".eng"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "eng",
      "scopeName": "source.eng",
      "path": "./syntaxes/eng.tmLanguage.json"
    }]
  }
}
