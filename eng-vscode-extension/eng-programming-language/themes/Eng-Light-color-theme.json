{
  "name": "Eng Light Theme",
  "type": "light",
  "colors": {
    "editor.background": "#ffffff",
    "editor.foreground": "#000000"
  },
  "tokenColors": [
    {
      "scope": "keyword.control.eng",
      "settings": {
        "foreground": "#0000ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "string.quoted.double.eng",
      "settings": {
        "foreground": "#a31515"
      }
    },
    {
      "scope": "constant.numeric.eng",
      "settings": {
        "foreground": "#098658"
      }
    },
    {
      "scope": "constant.language.boolean.eng",
      "settings": {
        "foreground": "#0000aa",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword.input.eng",
      "settings": {
        "foreground": "#b4009e"
      }
    },
    {
      "scope": "entity.name.function.eng",
      "settings": {
        "foreground": "#795e26",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "comment.line.double-slash.eng",
      "settings": {
        "foreground": "#6A9955",
        "fontStyle": "italic"
      }
    }
  ]
}
